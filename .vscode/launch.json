  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: semantic_kernel_chatbot",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "python": "/usr/local/bin/python",
      "program": "${workspaceFolder}/src/semantic_kernel_chatbot/app.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
